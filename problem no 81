class Solution {
    public boolean search(int[] nums, int target) {
        int start=0;
        int end=nums.length-1;
        while(start<=end){
            if(nums[start]==target || nums[end]== target){
                return true;
            }
            else if(target>nums[start]){
                start++;
            }
            else if(target<nums[end]){
                end--;
            }
            else{
                break;
            }
        }
        return false;
    }
}
